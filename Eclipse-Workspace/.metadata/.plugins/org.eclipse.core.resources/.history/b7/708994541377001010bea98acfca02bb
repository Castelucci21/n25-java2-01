public class Forca {
	private String PalavraSecreta;
	private String PalavraMascarada;
	private int Erros;
	private Random rnd;
	
	//Constructor
	public Forca() {
		rnd = new Random();
		int linhaEscolhida =rnd.nextInt(100);
		int linhaAtual = 0;
	}

}
